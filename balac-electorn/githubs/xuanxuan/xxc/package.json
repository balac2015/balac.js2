{
  "name": "xuanxuan",
  "productName": "喧喧",
  "version": "2.0.0",
  "description": "企业即时通讯平台",
  "main": "main.js",
  "scripts": {
    "hot-server": "cross-env NODE_ENV=development node --max_old_space_size=2096 -r babel-register ./build/server.js",
    "build-main": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config ./build/webpack.config.electron.js --progress --profile --colors",
    "build-renderer": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config ./build/webpack.config.production.js --progress --profile --colors",
    "build-renderer-debug": "cross-env NODE_ENV=debug node -r babel-register ./node_modules/webpack/bin/webpack --config ./build/webpack.config.debug.js --progress --profile --colors",
    "build-main-debug": "cross-env NODE_ENV=debug node -r babel-register ./node_modules/webpack/bin/webpack --config ./build/webpack.config.electron.js --progress --profile --colors",
    "build": "npm run build-main && npm run build-renderer",
    "build-debug": "npm run build-main-debug && npm run build-renderer-debug",
    "build-browser": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config ./build/webpack.config.browser.js --progress --profile --colors",
    "start": "cross-env NODE_ENV=production electron ./app/",
    "start-hot": "cross-env HOT=1 NODE_ENV=development electron -r babel-register -r babel-polyfill ./app/main.development",
    "start-hot-fast": "cross-env HOT=1 NODE_ENV=development SKIP_INSTALL_EXTENSIONS=1 electron -r babel-register -r babel-polyfill ./app/main.development",
    "postinstall": "npm run build-config - skipbuild && concurrently \"electron-builder install-app-deps\" \"node node_modules/fbjs-scripts/node/check-dev-engines.js package.json\"",
    "copy-browser-files": "cross-env NODE_ENV=development node --max_old_space_size=2096 -r babel-register ./build/package-browser.js",
    "build-config": "node --max_old_space_size=2096 -r babel-register ./build/build-config.js",
    "package": "npm run build-config - - -",
    "package-debug": "npm run build-config - - - debug",
    "package-mac": "npm run build-config - mac x64",
    "package-mac-debug": "npm run build-config - mac x64 debug",
    "package-win": "npm run build-config - win x64",
    "package-win-32": "npm run build-config - win ia32",
    "package-win-all": "npm run build-config - win all",
    "package-linux": "npm run build-config - linux x64",
    "package-linux-32": "npm run build-config - linux ia32",
    "package-win-debug": "npm run build-config - win x64 debug",
    "package-all": "npm run build-config - all all && npm run package-win-debug && npm run package-mac-debug",
    "package-beta": "npm run build-config - all all - beta && npm run package-win-debug beta && npm run package-mac-debug beta",
    "package-browser": "npm run build-config - skipbuild && npm run build-browser && npm run copy-browser-files",
    "publish-browser": "npm run build-config - skipbuild && npm run build-browser && npm run copy-browser-files -- --publish",
    "hot-server-browser": "npm run hot-server -- --target=browser",
    "start-browser": "http-server ./app -a 127.0.0.1 -p 8088",
    "docs": "./node_modules/.bin/esdoc"
  },
  "build": {
    "extends": "./build/electron-builder.json"
  },
  "bin": {
    "electron": "./node_modules/.bin/electron"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/easysoft/xuanxuan.git"
  },
  "author": {
    "name": "青岛易软天创网络科技有限公司",
    "email": "sunhao@cnezsoft.com"
  },
  "license": "ZPL 1.2",
  "bugs": {
    "url": "https://github.com/easysoft/xuanxuan/issues"
  },
  "keywords": [
    "im",
    "message",
    "electron",
    "react",
    "webpack",
    "react-hot"
  ],
  "homepage": "http://xuan.im",
  "company": "青岛易软天创网络科技有限公司",
